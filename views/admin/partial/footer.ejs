    $('.select2-selection').on('focus', function() {
        $(this).trigger('blur')
    })

    $('[data-plugin="customselect"]').select2({
        closeOnSelect: false,

    }).on("select2:open", function(e) {

        let intViewportHeight = window.innerHeight;
        console.log(intViewportHeight - 70)
        setTimeout(() => {
            $(".select2-results__group").each(function() {
                text = $(this).text().replace('*required*',
                    '<span class="badge badge-soft-danger p-1">SELEÃ‡ÃƒO OBRIGATÃ“RIA</span><br><small>Na pÃ¡gina do produto</small>'
                )
                $(this).html(text)
            });
        }, 100);

        $('.complete-select2').animate({
            'top': intViewportHeight - 70
        }, 100);

        window.addEventListener("resize", function() {

            if ($(".select2-container--open")[0]) {
                // Do something if class exists
                console.log(window.innerHeight - 70);
                $('.complete-select2').animate({
                    'top': window.innerHeight - 70
                }, 0);
            } else {
                // Do something if class does not exist
                console.log('nao existe')
            }


        });

        /*$('.complete-select2').animate({
            'bottom': '0px'
        }, 100);*/

    }).on("select2:close", function(e) {

        //log("select2:close", e); 
        console.log($('[data-plugin="customselect"]').select2('val').toString());
        $('.complete-select2').animate({
            'top': window.innerHeight + 180
        }, 100);
    })

    $('.finalizar').on('click', function() {

        $('body').click();
